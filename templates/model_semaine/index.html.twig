{% extends 'base.html.twig' %}

{% block title %}ModelSemaine index{% endblock %}

{% block body %}
    <h1>ModelSemaine index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>Semaine</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for model_semaine in model_semaines %}
            <tr>
                <td>{{ model_semaine.id }}</td>
                <td>{{ model_semaine.nom }}</td>
                <td>{{ model_semaine.FullDate|raw }}</td>
                <td>
                    <a href="{{ path('app_model_semaine_show', {'id': model_semaine.id}) }}">show</a>
                    <a href="{{ path('app_model_semaine_edit', {'id': model_semaine.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_model_semaine_new') }}">Create new</a>
{% endblock %}
